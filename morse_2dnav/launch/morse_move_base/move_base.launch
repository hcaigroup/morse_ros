<launch>
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <remap from="/base_scan" to="/Jido/Sick"/>
    <remap from="/cmd_vel" to="/Jido/Motion_Controller"/>
  
    <rosparam file="$(find morse_2dnav)/launch/morse_move_base/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find morse_2dnav)/launch/morse_move_base/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find morse_2dnav)/launch/morse_move_base/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find morse_2dnav)/launch/morse_move_base/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find morse_2dnav)/launch/morse_move_base/base_local_planner_params.yaml" command="load" />
  </node>

</launch>
